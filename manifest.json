{
  "name": "gptBlock",
  "description": "A chrome extenion built to monitor ChatGpt activity.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
    "permissions": [
      "sidePanel"
    ]
  }
  